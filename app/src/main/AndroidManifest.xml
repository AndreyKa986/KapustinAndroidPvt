<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="by.letum8658.homework">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="HomeWork"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">

        <activity android:name=".main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".dz0.Dz0Activity" />

        <activity android:name=".dz1.Dz1Activity" />

        <activity android:name=".dz2.Dz2ClassActivity" />
        <activity android:name=".dz2.Dz2HomeActivity" />

        <activity android:name=".dz3.Dz3Activity" />

        <activity android:name=".dz4.Dz4Activity" />

        <activity android:name=".dz5.Dz5FirstActivity" />
        <activity android:name=".dz5.Dz5SecondActivity" />

        <activity
            android:name=".dz6.Dz6StudentListActivity"
            android:launchMode="singleTask"
            android:windowSoftInputMode="stateHidden" />
        <activity android:name=".dz6.Dz6StudentDetailsActivity" />
        <activity android:name=".dz6.Dz6StudentEditActivity" />

        <activity
            android:name=".dz8.Dz8Activity"
            android:launchMode="singleTask"
            android:windowSoftInputMode="stateHidden" />
    </application>
</manifest>
